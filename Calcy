pragma solidity 0.8.19;
contract calculator{
int public result;
 function add(int a,int b)public
 {
 result=a+b;
 }
 function sub(int a,int b)public
 {
 result=a-b;
 }
 function mul(int a,int b)public
 {
 result=a*b;
 }
 function div(int a,int b)public
 {
 result=a/b;
 }
 function getresult()public view returns(int)
 {
 return result;
 }
} 
